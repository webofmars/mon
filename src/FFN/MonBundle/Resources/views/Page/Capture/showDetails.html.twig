{% extends 'FFNMonBundle::layout.html.twig' %}

{% block body -%}
<div class="container">
    <div class="row">
        <h1>{{ "mon_capture_detail_title" | trans }}</h1>
    </div>
    <div class="row">
        <table class="table table-striped table-hover table-bordered">
                <tr>
                    <th>Isconnectiontimeout</th>
                    <td>{{ entity.isConnectionTimeout }}</td>
                </tr>
                <tr>
                    <th>Isresponsetimeout</th>
                    <td>{{ entity.isResponseTimeout }}</td>
                </tr>
                <tr>
                    <th>Validators</th>
                    <td>{{ entity.validators }}</td>
                </tr>
                <tr>
                    <th>Content</th>
                    <td>{{ entity.content }}</td>
                </tr>
        </table>
    </div>
</div>
{% endblock %}
