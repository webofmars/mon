<div class="container main_page top_50">
  <form action="{{ path('fos_user_registration_register') }}" {{ form_enctype(form) }} method="POST" class="fos_user_registration_register form-signin">
    <h2 class="form-signin-heading">{{ 'mon_register'|trans({}, 'messages') }}</h2>
    {{ form_widget(form) }}
    <div>
        <input type="submit" value="{{ 'registration.submit'|trans({}, 'FOSUserBundle') }}" class="btn btn-primary btn-large" />
    </div>
  </form>
</div>